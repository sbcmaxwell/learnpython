    "# 以二进制写入模式打开文件\n",
    "with open('example.bin', 'wb') as file:\n",
    "    # 写入一些二进制数据\n",
    "    file.write(b'Hello World')\n",
    "\n",
    "# 以二进制读取模式打开相同的文件\n",
    "with open('example.bin', 'rb') as file:\n",
    "    # 读取数据\n",
    "    data = file.read()\n",
    "    print(data)\n",
    "\n",
    "    # 打印读取的字节类型数据\n",
    "    print('Type of data:', type(data))\n"
   ]
